#, c-format
msgid "Original size (%d) is smaller than the requested size (%d)"
msgstr "Oryginalny rozmiar (%d) jest mniejszy niż podany rozmiar (%d)."

#, c-format
msgid "ppm thumb mismatch, height %d, width %d, while buffer %d."
msgstr ""
"Niezgodność miniatury ppm, wysokość %d, szerokość %d, podczas gdy bufor  %d."

#, c-format
msgid "JPEG thumb height %d different than expected %d."
msgstr "Wysokość miniatury JPEG %d inna niż oczekiwana %d."

#, c-format
msgid "JPEG thumb width %d different than expected %d."
msgstr "Szerokość miniatury JPEG %d inna niż oczekiwana %d."

